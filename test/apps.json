{
  "services": [
    {
      "name": "static",
      "subdomains": [
        {
          "name": "www",
          "enabled": "true",
          "baseUrl": "static.com",
          "enableSsl": false,
          "port": 80
        }
      ],
      "origin": {
        "type": "local",
        "root": "/etc/lurch/test/static/"
      }
    },
    {
      "name": "dynamic",
      "subdomains": [
        {
          "name": "www",
          "enabled": "true",
          "baseUrl": "dynamic.com",
          "enableSsl": false,
          "port": 80
        }
      ],
      "origin": {
        "type": "remote",
        "host": "localhost",
        "port": 80,
        "headers": {
          "Host": "www.static.com"
        }
      }
    },
    {
      "name": "static-advanced",
      "subdomains": [
        {
          "name": "advanced",
          "enabled": "true",
          "baseUrl": "static.com",
          "enableSsl": false,
          "port": 80
        }
      ],
      "origin": {
        "type": "local",
        "root": "/etc/lurch/test/static/",
        "fallbacks": [
          "/index.html;"
        ],
        "errorPages": {
          "404": {
            "file": "/404.html"
          }
        },
        "pathRules": [
          {
            "type": "prefix",
            "path": "/api/",
            "stripPath": true,
            "origin": {
              "type": "www.",
              "host": "static.com",
              "port": 80
            }
          }
        ]
      }
    }
  ]
}

