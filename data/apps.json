{
  "defaultBaseUrl": "foo.bar",
  "authRequestUpstream": "https://auth.com",
  "authRequestRedirect": "https://login.com",
  "authRequestCookie": "mycookie",
  "services": [
    {
      "name": "an-upstream",
      "subdomains": [
        {
          "name": "sub1",
          "enabled": "true",
          "baseUrl": "foo.bar",
          "enableSsl": false,
          "enableClientCerts": false,
          "enableSso": true,
          "port": 80
        },
        {
          "name": "sub2",
          "enabled": "true",
          "baseUrl": "foo.bar2",
          "enableSsl": false,
          "enableClientCerts": false,
          "enableSso": false,
          "port": 80
        }
      ],
      "origin": {
        "type": "remote",
        "port": 9000,
        "host": "an.upstream.com"
      }
    }
  ]
}
